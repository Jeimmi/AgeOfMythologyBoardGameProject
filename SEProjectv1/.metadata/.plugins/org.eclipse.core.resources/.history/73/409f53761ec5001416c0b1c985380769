import java.util.ArrayList;
import java.util.Random;
import java.util.Queue;
public class Game extends UserInterface implements InitializeGame, RandomSelection{
	
	protected Player player1;
	protected Player player2;
	protected Player player3;	
	protected Player activePlayer;
	protected ArrayList<ProductionTile> productionPool;
	protected int[] bank;
	protected RandomSelection<?> selector;
	
	
	Game() {
		player1 = new Player(true);
		player2 = new Player(false);
		player3 = new Player(false);
		productionPool  = new ArrayList<ProductionTile>(); 
		bank = new int[5];
	}
	public void initialize(){
	}
	
	public static void main(String [] args){
		Game game = new Game();
		RandomSelection<Card> selection = new RandomSelection<Card>(game.player1.randomDeck);
		Card temp = selection.getRandomFromList();
	}
}
